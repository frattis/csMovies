@model IEnumerable<Dominio.Entidades.Release>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Releases</h2>

<table>
    <tr>
        <th>
            Nome
        </th>
        <th>
            LegendaSincronizada
        </th>
        <th>
            PossuiLegenda
        </th>
        <th>
            JaAssistido
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LegendaSincronizada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PossuiLegenda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JaAssistido)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
